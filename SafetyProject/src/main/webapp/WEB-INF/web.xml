<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>SafetyProject</display-name>

	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>

	<!-- Servlet Declaration -->
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>com.example.LogoutServlet</servlet-class>
	</servlet>

	<!-- Servlet Mapping -->
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/LogoutServlet</url-pattern>
	</servlet-mapping>
	
	<!-- Servlet Declaration -->
	<servlet>
		<servlet-name>fetchInspectorList</servlet-name>
		<servlet-class>com.example.fetchInspectorList</servlet-class>
	</servlet>

	<!-- Servlet Mapping -->
	<servlet-mapping>
		<servlet-name>fetchInspectorList</servlet-name>
		<url-pattern>/fetchInspectorList</url-pattern>
	</servlet-mapping>

</web-app>